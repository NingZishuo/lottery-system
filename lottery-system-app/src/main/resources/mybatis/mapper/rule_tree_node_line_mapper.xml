<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lottery.infrastructure.persistent.dao.IRuleTreeNodeLineDao">

    <resultMap id="dataMap" type="com.lottery.infrastructure.persistent.po.RuleTreeNodeLine">
        <id column="id" property="id"/>
        <result column="tree_id" property="treeId"/>
        <result column="rule_node_from" property="ruleNodeFrom"/>
        <result column="rule_node_to" property="ruleNodeTo"/>
        <result column="rule_limit_type" property="ruleLimitType"/>
        <result column="rule_logic_check_type" property="ruleLogicCheckType"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
    </resultMap>

    <select id="queryRuleTreeNodeLineListByTreeId" parameterType="java.lang.String" resultMap="dataMap">
        select *
        from rule_tree_node_line
        where tree_id = #{treeId}
    </select>
    <select id="queryRuleTreeNodeLineList" resultMap="dataMap">
        select * from rule_tree_node_line
        <where>
            <if test="treeId != null and treeId != ''">
                 tree_id = #{treeId}
            </if>
            <if test="ruleNodeFrom != null and ruleNodeFrom != ''">
                and rule_node_from = #{ruleNodeFrom}
            </if>
            <if test="ruleNodeTo != null and ruleNodeTo != ''">
                and rule_node_to = #{ruleNodeTo}
            </if>
            <if test="ruleLimitType != null and ruleLimitType != ''">
                and rule_limit_type = #{ruleLimitType}
            </if>
            <if test="ruleLogicCheckType != null and ruleLogicCheckType != ''">
                and rule_logic_check_type = #{ruleLogicCheckType}
            </if>
        </where>

    </select>

</mapper>
